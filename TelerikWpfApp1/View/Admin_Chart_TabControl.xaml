<UserControl x:Class="Meiday.View.Admin_Chart_TabControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
             xmlns:uc="clr-namespace:Meiday.View"
             xmlns:local="clr-namespace:Meiday.View"
             mc:Ignorable="d" 
             d:DesignHeight="290" d:DesignWidth="470">
    <Grid>
        <dx:DXTabControl FontWeight="Bold">
            <dx:DXTabItem x:Name="chart1" Header="내과">
                <uc:Admin_Chart_Dep_per_Ages></uc:Admin_Chart_Dep_per_Ages>
            </dx:DXTabItem>
            <dx:DXTabItem x:Name="chart2" Header="안과">
                <uc:Admin_Chart_Dep_per_Ages1></uc:Admin_Chart_Dep_per_Ages1>
            </dx:DXTabItem>
            <dx:DXTabItem x:Name="chart3" Header="순환기내과">
                <uc:Admin_Chart_Dep_per_Ages2></uc:Admin_Chart_Dep_per_Ages2>
            </dx:DXTabItem>
        </dx:DXTabControl>
    </Grid>
</UserControl>
